<odoo>
    <record id="padilla" model="my_modules.teachers">
        <field name="name">Diana Padilla</field>
    </record>
    <record id="carroll" model="my_modules.teachers">
        <field name="name">Jody Carroll</field>
    </record>
    <record id="vaughn" model="my_modules.teachers">
        <field name="name">Lester Vaughn</field>
    </record>


    <record id="course0" model="product.template">
            <field name="name">Course 0</field>
            <field name="teacher_id" ref="my_modules.padilla"/>
            <field name="public_categ_ids" eval="[(4, ref('my_modules.category_courses'), False)]"/>
            <field name="is_published">True</field>
            <field name="list_price" type="float">0</field>
            <field name="type">service</field>
        </record>
        <record id="course1" model="product.template">
            <field name="name">Course 1</field>
            <field name="teacher_id" ref="my_modules.padilla"/>
            <field name="public_categ_ids" eval="[(4, ref('my_modules.category_courses'), False)]"/>
            <field name="is_published">True</field>
            <field name="list_price" type="float">0</field>
            <field name="type">service</field>
        </record>
        <record id="course2" model="product.template">
            <field name="name">Course 2</field>
            <field name="teacher_id" ref="my_modules.vaughn"/>
            <field name="public_categ_ids" eval="[(4, ref('my_modules.category_courses'), False)]"/>
            <field name="is_published">True</field>
            <field name="list_price" type="float">0</field>
            <field name="type">service</field>
        </record>
</odoo>